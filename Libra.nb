                    RowBox[{
                    RowBox[{"IdentityMatrix", "[", "n", "]"}], "+", 
                    RowBox[{"c", "*", 
                    RowBox[{
                    RowBox[{"(", "x", ")"}], "^", "i2"}]}]}]}], ";", 
                   RowBox[{"o", "=", "1"}]}]}], "]"}], ";", 
                "\[IndentingNewLine]", 
                RowBox[{"mt", "=", 
                 RowBox[{"Transform", "[", 
                  RowBox[{"m", ",", "t", ",", "x"}], "]"}]}], ";", 
                RowBox[{"(*", 
