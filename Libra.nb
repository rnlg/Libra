                    RowBox[{"ind1", ",", "ind2"}], "]"}], "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"x", ",", "x0", ",", 
                    RowBox[{"o", "-", "i2"}]}], "}"}]}], "]"}], "]"}], ",", 
                    "ByteCount"}], "]"}], ",", "vars"}], "]"}]}], ",", 
                    RowBox[{
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"i2", ">", "0"}], ",", 
                    RowBox[{"Print", "[", "\"\<Problem\>\"", "]"}]}], "]"}], 
                    ";", 
                    RowBox[{"Continue", "[", "]"}]}], ",", 
                    RowBox[{"{", 
                    RowBox[{"GaussSolve", "::", "inconsistent"}], "}"}]}], 
                   "]"}], ",", 
                  RowBox[{"{", 
                   RowBox[{"GaussSolve", "::", "inconsistent"}], "}"}]}], 
                 "]"}], ";", "\[IndentingNewLine]", 
                RowBox[{"m", "=", 
                 RowBox[{"Factor", "[", 
                  RowBox[{
