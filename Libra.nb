           RowBox[{
            RowBox[{"mp", "=", 
             RowBox[{"m", "=", 
              RowBox[{"r", "-", 
               RowBox[{
                RowBox[{"IdentityMatrix", "[", "l", "]"}], "*", "ev"}]}]}]}], 
            ";", "\[IndentingNewLine]", 
            RowBox[{"vecs", "=", 
             RowBox[{"NullSpace", "[", "mp", "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"While", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Length", "[", "vecs", "]"}], "<", "k"}], ",", 
              "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"mp", "=", 
                RowBox[{"ODot", "[", 
