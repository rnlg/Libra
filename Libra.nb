 RowBox[{
  RowBox[{"JDFast", "[", 
   RowBox[{
    RowBox[{"r_", "?", "SquareMatrixQ"}], ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"evs", ",", "vecs", ",", "vecs1", ",", "vecs2", ",", 
      RowBox[{"l", "=", 
       RowBox[{"Length", "[", "r", "]"}]}], ",", "m", ",", "mp"}], "}"}], ",",
     "\[IndentingNewLine]", 
    RowBox[{
